{
  "manifest_version": 2,
  "name": "Last.fm for Google Play Music",
  "version": "1.0",
  "author": "Ash Kyd",
  "description": "last.fm support for Google Play Music",
  "content_scripts": [
    {
      "matches": ["https://play.google.com/music/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "icons": {
    "100": "icons/raster.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/vector.svg"
    }
  }

}
