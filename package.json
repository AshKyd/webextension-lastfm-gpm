{
  "name": "webextension-lastfm-gpm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean;npm run copy && npm run browserify && npm run zip",
    "clean": "rm build -rf;rm build.zip",
    "copy": "cp content build -r && mkdir build/scripts",
    "browserify": "for f in scripts/*; do browserify $f -o build/$f& done;wait",
    "zip": "cd build && zip ../build.zip *",
    "watch": "watch 'npm run build' scripts content --wait=5"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "he": "^1.1.1",
    "last.fm.api": "git+ssh://git@github.com/AshKyd/last.fm.api.git"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "watch": "^1.0.2"
  }
}
